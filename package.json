{
  "name": "@owja/babel-css-inline-plugin",
  "version": "1.0.0-alpha.1",
  "description": "Babel plugin to inline CSS into JavaScript",
  "source": "src/plugin.ts",
  "main": "dist/plugin.js",
  "module": "dist/plugin.mjs",
  "types": "dist/plugin.d.ts",
  "scripts": {
    "build": "shx rm -rf dist/* && microbundle --format es,cjs --target node",
    "test": "jest",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --ext .ts ./src --fix",
    "prettier": "prettier src/**/*.ts --check",
    "prettier:fix": "prettier src/**/*.ts --write"
  },
  "keywords": [
    "babel",
    "css",
    "inline",
    "plugin",
    "styles",
    "stylesheets",
    "build",
    "parser"
  ],
  "author": "Hauke Broer",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/types": "^7.10.2",
    "@types/babel__core": "^7.1.8",
    "@types/babel__traverse": "^7.0.12",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-plugin-tester": "^9.2.0",
    "eslint": "^7.2.0",
    "eslint-plugin-jest": "^23.13.2",
    "jest": "^26.0.1",
    "microbundle": "^0.12.0",
    "prettier": "2.0.5",
    "shx": "^0.3.2",
    "slash": "^3.0.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "browserslist": [
    "IE > 9",
    "FF > 20",
    "CHROME > 20"
  ],
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.1"
  },
  "files": [
    "/dist",
    "!/dist/postcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owja/babel-css-inline-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/owja/babel-css-inline-plugin/issues"
  }
}
