{
  "name": "@owja/babel-css-inline-plugin",
  "version": "1.0.0-alpha.1",
  "description": "Babel plugin to inline CSS into JavaScript",
  "source": "src/plugin.ts",
  "main": "dist/plugin.js",
  "module": "dist/plugin.mjs",
  "types": "dist/plugin.d.ts",
  "scripts": {
    "build": "rm -rf dist/* && microbundle --format es,cjs --target node",
    "test": "jest",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --ext .ts ./src --fix",
    "prettier": "prettier src/**/*.ts --check",
    "prettier:fix": "prettier src/**/*.ts --write"
  },
  "keywords": [
    "babel",
    "css",
    "inline",
    "plugin",
    "styles",
    "stylesheets",
    "build",
    "parser"
  ],
  "author": "Hauke Broer",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/types": "^7.8.7",
    "@types/babel__core": "^7.1.6",
    "@types/babel__traverse": "^7.0.9",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-plugin-tester": "^8.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.1.0",
    "microbundle": "^0.12.0-next.8",
    "prettier": "1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    "IE > 9",
    "FF > 20",
    "CHROME > 20"
  ],
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.0"
  },
  "files": [
    "/dist",
    "!/dist/postcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owja/babel-css-inline-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/owja/babel-css-inline-plugin/issues"
  }
}
